{
    "name": "@navikt/ds-datepicker",
    "version": "1.0.4",
    "description": "Datepicker with input and calendar. Based on @navikt/ds-react and @navikt/ds-css",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "dev": "craco start --no-cache",
        "test": "jest --maxWorkers=2 --config ./jest/jest.config.js",
        "test-w": "jest --maxWorkers=2 --config ./jest/jest.config.js --watch",
        "build-lib": "npm-run-all clean-lib compile-lib-script compile-less",
        "clean-lib": "rimraf ./lib",
        "compile-less": "lessc ./src/datepicker/styles/datepicker.less lib/index.css",
        "compile-lib-script": "tsc -p tsconfig-build-lib.json",
        "build-dev-app": "craco build",
        "predeploy": "npm run build-dev-app",
        "deploy": "gh-pages -d build"
    },
    "dependencies": {
        "@navikt/ds-css": "^0.10.0",
        "@navikt/ds-react": "^0.11.0",
        "focus-trap-react": "^8.8.2",
        "less": "^4.1.2",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@craco/craco": "^6.3.0",
        "@navikt/fnrvalidator": "^1.1.4",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.0.2",
        "@types/node": "^16.11.1",
        "@types/react": "^17.0.30",
        "@types/react-dom": "^17.0.9",
        "@typescript-eslint/eslint-plugin": "^4.0.0",
        "@typescript-eslint/parser": "^4.0.0",
        "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
        "classnames": "^2.3.1",
        "core-js": "^3.18.3",
        "craco-less": "^1.20.0",
        "date-holidays": "^3.11.0",
        "dayjs": "^1.10.7",
        "enzyme": "^3.11.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.26.1",
        "gh-pages": "^3.2.3",
        "history": "^5.0.1",
        "intl": "^1.2.5",
        "jest": "^27.3.1",
        "jest-css-modules": "^2.1.0",
        "less-plugin-npm-import": "^2.1.0",
        "lodash.throttle": "^4.1.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.4.1",
        "react": "^17.0.2",
        "react-collapse": "^5.1.0",
        "react-day-picker": "^7.4.10",
        "react-dom": "^17.0.2",
        "react-intl": "^5.20.13",
        "react-modal": "^3.14.3",
        "react-router-dom": "^5.3.0",
        "react-scripts": "^4.0.3",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.7",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^5.0.0",
        "typescript": "^4.4.4"
    },
    "peerDependencies": {
        "@navikt/ds-css": "^0.9.1",
        "@navikt/ds-react": "^0.10.2",
        "classnames": "^2.3.x",
        "dayjs": "^1.10.x",
        "react": "17",
        "react-day-picker": "^7.4.x"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "files": [
        "package.json",
        "/lib"
    ],
    "directories": {
        "lib": "lib"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/navikt/ds-datepicker.git"
    },
    "author": "navikt",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/navikt/ds-datepicker/issues"
    },
    "homepage": "https://navikt.github.io/ds-datepicker",
    "publishConfig": {
        "access": "public"
    }
}
